  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test",
      "...": "..."
    }
  },
  "lint-staged": {
    "ui/**/**/**/**/**/*.js": [
      "standard",
      "git add"
    ],
    "domains/**/**/**/**/**/*.js": [
      "standard",
      "git add"
    ]
  },
